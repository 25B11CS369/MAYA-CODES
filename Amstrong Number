#include <stdio.h>
#include <math.h>
int isArmstrong(int num) 
{
    int original = num, sum = 0, digits = 0, temp;
    temp = num;
    while (temp != 0) {
        digits++;
        temp /= 10;
    }
    temp = num;
    while (temp != 0) {
        int digit = temp % 10;
        sum += pow(digit, digits);
        temp /= 10;
    }
    return sum == original;
}
int main() {
    int n;
    scanf("%d", &n)
    if (isArmstrong(n))
        printf("YES");
    else
        printf("NO");
    return 0;
}
